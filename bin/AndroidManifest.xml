        <activity
            android:name="com.huni.SelectActivity" ></activity>
        <activity android:name="com.huni.FirstActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>